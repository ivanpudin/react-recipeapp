{
  "recipes": [
    {
      "id": 1,
      "name": "Spaghetti Bolognese",
      "author": "Ivan",
      "country": "it",
      "ingredients": [
        {
          "value": "500g spaghetti"
        },
        {
          "value": "400g ground beef"
        },
        {
          "value": "1 onion, chopped"
        },
        {
          "value": "2 cloves of garlic, minced"
        },
        {
          "value": "400g canned tomatoes"
        },
        {
          "value": "2 tablespoons tomato paste"
        },
        {
          "value": "1 teaspoon dried oregano"
        },
        {
          "value": "1 teaspoon dried basil"
        },
        {
          "value": "Salt and pepper to taste"
        },
        {
          "value": "Parmesan cheese for serving"
        }
      ],
      "instructions": "1. Cook spaghetti according to package instructions.\n2. In a large pan, brown the ground beef, onion, and garlic.\n3. Add canned tomatoes, tomato paste, oregano, basil, salt, and pepper. Simmer for 20 minutes.\n4. Serve the Bolognese sauce over cooked spaghetti. Sprinkle with Parmesan cheese.\n5. Enjoy!"
    },
    {
      "id": 2,
      "name": "Chicken Tikka Masala",
      "author": "Ivan",
      "country": "in",
      "ingredients": [
        {
          "value": "500g boneless chicken, cut into cubes"
        },
        {
          "value": "1 cup plain yogurt"
        },
        {
          "value": "2 tablespoons tikka masala spice mix"
        },
        {
          "value": "1 tablespoon ginger-garlic paste"
        },
        {
          "value": "1 onion, finely chopped"
        },
        {
          "value": "2 tomatoes, pureed"
        },
        {
          "value": "1 cup heavy cream"
        },
        {
          "value": "2 tablespoons butter"
        },
        {
          "value": "Fresh cilantro leaves for garnish"
        },
        {
          "value": "Cooked rice or naan bread for serving"
        }
      ],
      "instructions": "1. In a bowl, mix yogurt, tikka masala spice mix, and ginger-garlic paste. Add chicken cubes and marinate for 1 hour.\n2. In a pan, heat butter and saut√© onion until golden brown.\n3. Add tomato puree and cook until oil separates.\n4. Add marinated chicken and cook until tender.\n5. Stir in heavy cream and simmer for 5 minutes.\n6. Garnish with cilantro leaves and serve with rice or naan bread.\n7. Enjoy!"
    },
    {
      "id": 3,
      "name": "Caesar Salad",
      "author": "Ivan",
      "country": "us",
      "ingredients": [
        {
          "value": "1 head of romaine lettuce, chopped"
        },
        {
          "value": "1 cup croutons"
        },
        {
          "value": "1/2 cup grated Parmesan cheese"
        },
        {
          "value": "1/4 cup Caesar dressing"
        },
        {
          "value": "2 tablespoons lemon juice"
        },
        {
          "value": "1 clove of garlic, minced"
        },
        {
          "value": "1/2 teaspoon Dijon mustard"
        },
        {
          "value": "Salt and pepper to taste"
        }
      ],
      "instructions": "1. In a large bowl, combine romaine lettuce, croutons, and Parmesan cheese.\n2. In a small bowl, whisk together Caesar dressing, lemon juice, garlic, Dijon mustard, salt, and pepper.\n3. Drizzle the dressing over the salad and toss to coat.\n4. Serve immediately.\n5. Enjoy!"
    },
    {
      "id": 4,
      "name": "Tacos",
      "author": "Ivan",
      "country": "mx",
      "ingredients": [
        {
          "value": "500g ground beef"
        },
        {
          "value": "1 onion, chopped"
        },
        {
          "value": "2 cloves of garlic, minced"
        },
        {
          "value": "1 tablespoon chili powder"
        },
        {
          "value": "1 teaspoon cumin"
        },
        {
          "value": "1 teaspoon paprika"
        },
        {
          "value": "Salt and pepper to taste"
        },
        {
          "value": "Taco shells or tortillas"
        },
        {
          "value": "Taco toppings (lettuce, tomatoes, cheese, salsa, etc.)"
        }
      ],
      "instructions": "1. In a pan, brown the ground beef with onion and garlic.\n2. Add chili powder, cumin, paprika, salt, and pepper. Cook until meat is cooked through.\n3. Warm up taco shells or tortillas.\n4. Fill the shells or tortillas with the cooked ground beef.\n5. Add your desired taco toppings.\n6. Enjoy!"
    }
